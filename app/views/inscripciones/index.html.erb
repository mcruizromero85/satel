<section class="FrmRegistrarTorneo">
  <%= render partial: "reutilizable/ficha_presentacion_torneo_resumido", object: @torneo, as: :torneo %>
  <% if !@current_gamer.esta_inscrito(@torneo) %>
    <center><a  href="/inscripciones/new/<%=@torneo.id%>"><button class="link_button" type="button">Inscribirme al torneo</button></a></center>
  <% end %>
  
  <Section class="Lista_Inscritos" >
  <h1>Lista de inscritos</h1>
        <script>
      $(function() {
        $( "#accordion" ).accordion({
          collapsible: true     
        });        
      });
      $( ".selector" ).accordion( "option", "header", "article .Lista_Inscritos_Item" );
      $( ".selector" ).accordion( "option", "active", false );
      </script>
  <% if !@mensaje_inscripcion.nil? %>
  <script>

  </script>
  <div id="dialog-confirm" title="InscripciÃ³n confirmada">
    <p><span class="ui-icon ui-icon-alert" style="float:left; margin:0 7px 20px 0;"></span><%=@mensaje_inscripcion%></p>
  </div>
  <% end %>
  <article id="accordion" class="lista_Inscritos_contenido">
     <% @inscripciones.each do |inscripcion| %>       
         <article class = "Lista_Inscritos_Item">
           <article>
            <!-- <img class="Lista_Inscritos_item_UsFb" src="<%= inscripcion.gamer.authentications[0].icono %>" height='35px' width='35px'> -->
           </article>
           <article>
            <p class="Lista_Inscritos_item_nick"><b><%= inscripcion.nick %></b></p>
           </article>
            <article>
              <p class="Lista_Inscritos_item_nick">
                <b><%= inscripcion.estado %></b>
                </p>
              <% if inscripcion.estado == "Confirmado" %>
              <% end%>
            </article>
            <article>
              <img class="confirmacion_check" src="<%= image_path('check_confirmado.png')%>" height='35px' width='35px' > 
            </article>
         </article>
         <article class="miembros_equipo">
           <%= render partial: "hots_formularios/show", object: inscripcion, as: :inscripcion %>
         </article>
      <% end %>
    <br/>

  </article>
  </section>
      <center>
        <% if @mensaje_inscripcion.nil? %>
          <a class="Enlace_detalle_registrarTorneo" href="javascript:goBack();">Volver</a></center>
        <% else %>
          <a class="Enlace_detalle_registrarTorneo" href="/">Volver</a>
        <% end %>
      </center>
    <br/>
</section>
